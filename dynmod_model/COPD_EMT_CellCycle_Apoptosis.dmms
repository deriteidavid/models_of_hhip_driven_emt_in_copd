  title = {Nonstochastic Reprogramming from a Privileged Somatic Cell State},
  author = {Guo, Shangqin and Zi, Xiaoyuan and Schulz, Vincent P. and Cheng, Jijun and Zhong, Mei and Koochaki, Sebastian H. J. and Megyola, Cynthia M. and Pan, Xinghua and Heydari, Kartoosh and Weissman, Sherman M. and Gallagher, Patrick G. and Krause, Diane S. and Fan, Rong and Lu, Jun},
  year = {2014},
