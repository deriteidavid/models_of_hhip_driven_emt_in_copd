@article{welch-reardonRolePartialEndothelialmesenchymal2015,
  ids = {welch-reardonRolePartialEndothelialmesenchymal2015a},
  title = {A Role for Partial Endothelial-Mesenchymal Transitions in Angiogenesis?},
  author = {{Welch-Reardon}, Katrina M. and Wu, Nan and Hughes, Christopher C. W.},
