@article{gervaisCleavageCDKInhibitor1998,
  title = {Cleavage of {{CDK}} Inhibitor P21({{Cip1}}/{{Waf1}}) by Caspases Is an Early Event during {{DNA}} Damage-Induced Apoptosis},
  author = {Gervais, J. L. and Seth, P. and Zhang, H.},
  year = {1998},
  month = jul,
  journal = {The Journal of Biological Chemistry},
  volume = {273},
  number = {30},
  pages = {19207--19212},
  issn = {0021-9258},
  doi = {10.1074/jbc.273.30.19207},
  abstract = {Activation of the p53-mediated DNA damage response induces either G1 cell cycle arrest or apoptosis. The G1 cell cycle arrest is in part caused by the p53-dependent transcriptional activation of the CDK inhibitor, p21(Cip1/Waf1). We report here that human p21 protein is rapidly induced but selectively cleaved during the apoptotic response to gamma-irradiation. Such an event occurred early, well before the morphological appearance of apoptosis. Ectopical expression of p53 in tumor cells alone could induce p21 expression, followed by p21 cleavage and apoptosis. The cleavage of p21 could be reproduced in extracts prepared from irradiated cells or by recombinant caspase-3, suggesting that a caspase-like activity is responsible for this cleavage. p21 binds independently to both CDK2 and proliferation cell nuclear antigen (PCNA). Our studies indicated that p21 cleavage by the caspase-like activity specifically abolished its interaction with PCNA, suggesting that p21 cleavage may interfere with normal PCNA-dependent repair. Our data suggest that p21 may serve as a critical checkpoint regulator for both cell cycle arrest and apoptosis during the p53-mediated DNA damage response. Manipulation of the checkpoint regulators involved in cell cycle arrest and apoptosis may thus provide a novel strategy to cancer therapy.},
  langid = {english},
  pmid = {9668108},
  keywords = {Apoptosis,Caspase 3,Caspases,CDC2-CDC28 Kinases,Cyclin A,Cyclin-Dependent Kinase 2,Cyclin-Dependent Kinase Inhibitor p21,Cyclin-Dependent Kinases,Cyclins,Cysteine Endopeptidases,DNA Damage,Humans,Molecular Weight,Peptide Fragments,Proliferating Cell Nuclear Antigen,Protein-Serine-Threonine Kinases,Tumor Cells; Cultured,Tumor Suppressor Protein p53},
  file = {/Users/eregan/Zotero/storage/BUM3PBVK/Gervais et al. - 1998 - Cleavage of CDK inhibitor p21(Cip1Waf1) by caspas.pdf;/Users/eregan/Zotero/storage/E2Y8HF2B/Gervais et al. - 1998 - Cleavage of CDK inhibitor p21(Cip1Waf1) by caspas.pdf;/Users/eregan/Zotero/storage/M39CG8K4/Gervais et al. - 1998 - Cleavage of CDK inhibitor p21(Cip1Waf1) by caspas.pdf}
