  author = {Tan, Y. and Demeter, M. R. and Ruan, H. and Comb, M. J.},
  year = {2000},
  month = aug,
  journal = {The Journal of Biological Chemistry},
  volume = {275},
  number = {33},
  pages = {25865--25869},
  issn = {0021-9258},
  doi = {10.1074/jbc.M004199200},
  abstract = {The BH3 domain of BAD mediates its death-promoting activities via heterodimerization to the Bcl-XL family of death regulators. Growth and survival factors inhibit the death-promoting activity of BAD by stimulating phosphorylation at multiple sites including Ser-112 and Ser-136. Phosphorylation at these sites promotes binding of BAD to 14-3-3 proteins, sequestering BAD away from the mitochondrial membrane where it dimerizes with Bcl-XL to exert its killing effects. We report here that the phosphorylation of BAD at Ser-155 within the BH3 domain is a second phosphorylation-dependent mechanism that inhibits the death-promoting activity of BAD. Protein kinase A, RSK1, and survival factor signaling stimulate phosphorylation of BAD at Ser-155, blocking the binding of BAD to Bcl-XL. RSK1 phosphorylates BAD at both Ser-112 and Ser-155 and rescues BAD-mediated cell death in a manner dependent upon phosphorylation at both sites.},
  langid = {english},
  pmid = {10837486},
  keywords = {14-3-3 Proteins,Amino Acid Sequence,bcl-Associated Death Protein,bcl-X Protein,Carrier Proteins,Cell Line,Cell Survival,Cyclic AMP-Dependent Protein Kinases,Cytoplasm,DNA; Complementary,Glutathione Transferase,Humans,Immunoblotting,Mitochondria,Mitogen-Activated Protein Kinases,Molecular Sequence Data,Mutagenesis; Site-Directed,Phosphorylation,Plasmids,Precipitin Tests,Protein Structure; Tertiary,Proteins,Proto-Oncogene Proteins c-bcl-2,Recombinant Fusion Proteins,Ribosomal Protein S6 Kinases; 90-kDa,Serine,Tyrosine 3-Monooxygenase},
  file = {/Users/eregan/Zotero/storage/A2RGD5EN/Tan et al. - 2000 - BAD Ser-155 phosphorylation regulates BADBcl-XL i.pdf;/Users/eregan/Zotero/storage/FJWQVYP5/Tan et al. - 2000 - BAD Ser-155 phosphorylation regulates BADBcl-XL i.pdf;/Users/eregan/Zotero/storage/LMH4AAZH/Tan et al. - 2000 - BAD Ser-155 phosphorylation regulates BADBcl-XL i.pdf}
