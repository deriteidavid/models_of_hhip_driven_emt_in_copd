@article{paoliAnoikisMolecularPathways2013,
  title = {Anoikis Molecular Pathways and Its Role in Cancer Progression},
  author = {Paoli, Paolo and Giannoni, Elisa and Chiarugi, Paola},
  year = {2013},
  month = dec,
  journal = {Biochimica Et Biophysica Acta},
  volume = {1833},
  number = {12},
  pages = {3481--3498},
  issn = {0006-3002},
  doi = {10.1016/j.bbamcr.2013.06.026},
  abstract = {Anoikis is a programmed cell death induced upon cell detachment from extracellular matrix, behaving as a critical mechanism in preventing adherent-independent cell growth and attachment to an inappropriate matrix, thus avoiding colonizing of distant organs. As anchorage-independent growth and epithelial-mesenchymal transition, two features associated with anoikis resistance, are vital steps during cancer progression and metastatic colonization, the ability of cancer cells to resist anoikis has now attracted main attention from the scientific community. Cancer cells develop anoikis resistance due to several mechanisms, including change in integrins' repertoire allowing them to grow in different niches, activation of a plethora of inside-out pro-survival signals as over-activation of receptors due to sustained autocrine loops, oncogene activation, growth factor receptor overexpression, or mutation/upregulation of key enzymes involved in integrin or growth factor receptor signaling. In addition, tumor microenvironment has also been acknowledged to contribute to anoikis resistance of bystander cancer cells, by modulating matrix stiffness, enhancing oxidative stress, producing pro-survival soluble factors, triggering epithelial-mesenchymal transition and self-renewal ability, as well as leading to metabolic deregulations of cancer cells. All these events help cancer cells to inhibit the apoptosis machinery and sustain pro-survival signals after detachment, counteracting anoikis and constituting promising targets for anti-metastatic pharmacological therapy. This article is part of a Special Section entitled: Cell Death Pathways.},
  langid = {english},
  pmid = {23830918},
  keywords = {AMP activated protein kinase,AMPK,Animals,Anoikis,Cancer,Cytoprotection,Disease Progression,ECM,EGFR,EMT,epidermal growth factor receptor,epithelial–mesenchymal transition,ERK,extracellular matrix,extracellular signal-regulated kinase,FAK,FLICE inhibitory protein,FLIP,focal adhesion kinase,glycogen synthase kinase-3,GSK-3,hepatocyte growth factor,HGF,HIF,Humans,hypoxia-inducible factor,IKK,IL,ILK,integrin-linked kinase,interleukin,IκB kinase,MAPK,Mcl-1,mesenchymal epithelial transition,MET,Metabolism,metalloproteinase,MicroRNA,mitogen activated protein kinase,MMP,myeloid cell leukemia sequence 1,NADPH oxidase,Neoplasm Metastasis,Neoplasms,NFκB,Nox,nuclear factor-κB,OMM,outer mitochondrial membrane,PDGFR,PDK,pentose phosphate pathway,PERK,phosphatase and tensin homolog,phosphatidylinositol 3;4;5-triphosphate,phosphoinositide-3-OH kinase,PI3K,PIP3,PK-M2,PKB,platelet-derived growth factor receptor,PPP,protein kinase B,protein kinase like endoplasmic reticulum kinase,PTEN,pyruvate dehydrogenase kinase,pyruvate kinase isoform-2,reactive oxygen species,Reactive oxygen species,receptor tyrosine kinases,ROS,RTKs,Signal Transduction,SMA,TNFR,TrkB,tumor necrosis factor receptor,tyrosine kinase receptor B,vascular endothelial growth factor receptor,VEGFR,α-smooth muscle actin},
  file = {/Users/eregan/Zotero/storage/H8JK83K4/Paoli et al. - 2013 - Anoikis molecular pathways and its role in cancer .pdf}
}

