  author = {Sarosiek, Kristopher A. and Chi, Xiaoke and Bachman, John A. and Sims, Joshua J. and Montero, Joan and Patel, Luv and Flanagan, Annabelle and Andrews, David W. and Sorger, Peter and Letai, Anthony},
  year = {2013},
  month = sep,
  journal = {Molecular Cell},
  volume = {51},
  number = {6},
  pages = {751--765},
  issn = {1097-4164},
  doi = {10.1016/j.molcel.2013.08.048},
  abstract = {Apoptosis is a highly regulated form of cell death that controls normal homeostasis as well as the antitumor activity of many chemotherapeutic agents. Commitment to death via the mitochondrial apoptotic pathway requires activation of the mitochondrial pore-forming proteins BAK or BAX. Activation can be effected by the activator BH3-only proteins BID or BIM, which have been considered to be functionally redundant in this role. Herein, we show that significant activation preferences exist between these proteins: BID preferentially activates BAK while BIM preferentially activates BAX. Furthermore, we find that cells lacking BAK are relatively resistant to agents that require BID activation for maximal induction of apoptosis, including topoisomerase inhibitors and TRAIL. Consequently, patients with tumors that harbor a loss of BAK1 exhibit an inferior response to topoisomerase inhibitor treatment in the clinic. Therefore, BID and BIM have nonoverlapping roles in the induction of apoptosis via BAK and BAX, affecting chemotherapy response.},
  langid = {english},
  pmcid = {PMC4164233},
  pmid = {24074954},
  keywords = {Apoptosis,Apoptosis Regulatory Proteins,bcl-2 Homologous Antagonist-Killer Protein,bcl-2-Associated X Protein,Bcl-2-Like Protein 11,BH3 Interacting Domain Death Agonist Protein,Female,Gene Expression Regulation; Neoplastic,HeLa Cells,Humans,Membrane Proteins,Mitochondria,Neoplasm Staging,Ovarian Neoplasms,Proto-Oncogene Proteins,Topoisomerase Inhibitors,Transcriptional Activation},
  file = {/Users/eregan/Zotero/storage/49MIBU3Z/Sarosiek et al. - 2013 - BID preferentially activates BAK while BIM prefere.pdf;/Users/eregan/Zotero/storage/9GHETDUD/Sarosiek et al. - 2013 - BID preferentially activates BAK while BIM prefere.pdf;/Users/eregan/Zotero/storage/KBMWCVAA/Sarosiek et al. - 2013 - BID preferentially activates BAK while BIM prefere.pdf}
