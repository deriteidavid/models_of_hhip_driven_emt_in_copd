@article{haoTumorSuppressorLATS12008,
  title = {Tumor Suppressor {{LATS1}} Is a Negative Regulator of Oncogene {{YAP}}},
  author = {Hao, Yawei and Chun, Alex and Cheung, Kevin and Rashidi, Babak and Yang, Xiaolong},
