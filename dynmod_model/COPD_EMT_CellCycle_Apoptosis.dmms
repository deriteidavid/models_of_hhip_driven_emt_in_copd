@article{noritakeIQGAP1KeyRegulator2005,
  title = {{{IQGAP1}}: A Key Regulator of Adhesion and Migration},
  shorttitle = {{{IQGAP1}}},
  author = {Noritake, Jun and Watanabe, Takashi and Sato, Kazumasa and Wang, Shujie and Kaibuchi, Kozo},
