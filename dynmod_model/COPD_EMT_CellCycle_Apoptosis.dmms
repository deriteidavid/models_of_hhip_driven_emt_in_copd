@article{baiTheoreticalExperimentalEvidence2003,
  title = {Theoretical and Experimental Evidence for Hysteresis in Cell Proliferation},
  author = {Bai, Shaochun and Goodrich, David and Thron, C. Dennis and Tecarro, Edwin and Obeyesekere, Mandri},
