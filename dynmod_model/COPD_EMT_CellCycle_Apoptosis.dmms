@article{oconnorBiomechanicsTGFvinducedEpithelialmesenchymal2014,
  ids = {oconnorBiomechanicsTGFvinducedEpithelialmesenchymal2014a},
  title = {Biomechanics of {{TGF$\beta$-induced}} Epithelial-Mesenchymal Transition: Implications for Fibrosis and Cancer},
  shorttitle = {Biomechanics of {{TGF$\beta$-induced}} Epithelial-Mesenchymal Transition},
  author = {O'Connor, Joseph W. and Gomez, Esther W.},
  year = {2014},
  journal = {Clinical and Translational Medicine},
  volume = {3},
  pages = {23},
  issn = {2001-1326},
  doi = {10.1186/2001-1326-3-23},
  abstract = {Fibrosis, a disease that results in loss of organ function, contributes to a significant number of deaths worldwide and sustained fibrotic activation has been suggested to increase the risk of developing cancer in a variety of tissues. Fibrogenesis and tumor progression are regulated in part through the activation and activity of myofibroblasts. Increasing evidence links myofibroblasts found within fibrotic lesions and the tumor microenvironment to a process termed epithelial-mesenchymal transition (EMT), a phenotypic change in which epithelial cells acquire mesenchymal characteristics. EMT can be stimulated by soluble signals, including transforming growth factor (TGF)-{$\beta$}, and recent studies have identified a role for mechanical cues in directing EMT. In this review, we describe the role that EMT plays in fibrogenesis and in the progression of cancer, with particular emphasis placed on biophysical signaling mechanisms that control the EMT program. We further describe specific TGF{$\beta$}-induced intracellular signaling cascades that are affected by cell- and tissue-level mechanics. Finally, we highlight the implications of mechanical induction of EMT on the development of treatments and targeted intervention strategies for fibrosis and cancer.},
  langid = {english},
  pmcid = {PMC4114144},
  pmid = {25097726},
  keywords = {Biomechanics,Cancer,Cell shape,Epithelial-mesenchymal transition,Fibrosis,Matrix rigidity,Mechanotransduction,Myofibroblast,Transforming growth factor},
  file = {/Users/eregan/Zotero/storage/99VNXUF5/O'Connor and Gomez - 2014 - Biomechanics of TGFβ-induced epithelial-mesenchyma.pdf;/Users/eregan/Zotero/storage/F6UZ9U43/O'Connor and Gomez - 2014 - Biomechanics of TGFβ-induced epithelial-mesenchyma.pdf}
