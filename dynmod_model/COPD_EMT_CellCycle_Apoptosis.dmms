@article{liAdaptorProteinAnaphase2008,
  title = {The Adaptor Protein of the Anaphase Promoting Complex {{Cdh1}} Is Essential in Maintaining Replicative Lifespan and in Learning and Memory},
  author = {Li, Min and Shin, Yong-Hyun and Hou, Lingfei and Huang, Xingxu and Wei, Zhubo and Klann, Eric and Zhang, Pumin},
  year = {2008},
  month = sep,
  journal = {Nature Cell Biology},
  volume = {10},
  number = {9},
  pages = {1083--1089},
  issn = {1465-7392},
  doi = {10.1038/ncb1768},
  abstract = {The anaphase promoting complex (APC) or cyclosome is a multisubunit E3 ubiquitin ligase. Cdc20 (fizzy (fzy)) or p55CDC, and Cdh1 (Hct1, srw1 or fizzy-related 1 (fzr1)) encode two adaptor proteins that bring substrates to the APC. Both APC-Cdc20 and APC-Cdh1 have been implicated in the control of mitosis through mediating ubiquitination of mitotic regulators, such as cyclin B1 and securin. However, the importance of Cdh1 function in vivo and whether its function is redundant with that of Cdc20 are unclear. Here we have analysed mice lacking Cdh1. We show that Cdh1 is essential for placental development and that its deficiency causes early lethality. Cdhl-deficient mouse embryonic fibroblasts (MEFs) entered replicative senescence prematurely because of stabilization of Ets2 and subsequent activation of p6(Ink4a) expression. These results have uncovered an unexpected role of the APC in maintaining replicative lifespan of MEFs. Further, Cdh1 heterozygous mice show defects in late-phase long-term potentiation (L-LTP) in the hippocampus and are deficient in contextual fear-conditioning, suggesting that Cdh1 has a role in learning and memory.},
  langid = {english},
  pmcid = {PMC2914158},
  pmid = {19160489},
  keywords = {Anaphase-Promoting Complex-Cyclosome,Animals,Cdh1 Proteins,Cell Cycle Proteins,Cellular Senescence,Cyclin-Dependent Kinase Inhibitor p16,Embryo Loss,Fibroblasts,Heterozygote,In Vitro Techniques,Memory,Mice,Protein Stability,Proto-Oncogene Protein c-ets-2,Substrate Specificity,Ubiquitin-Protein Ligase Complexes},
