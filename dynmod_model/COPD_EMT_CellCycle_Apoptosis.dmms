  pmid = {19185590},
  keywords = {cdc25 Phosphatases,Cell Cycle Proteins,Cell Division,Cell Nucleus,Cells; Cultured,Cytoplasm,DNA Damage,Fluorescent Antibody Technique,G2 Phase,Humans,Mitosis,Protein-Serine-Threonine Kinases,Proto-Oncogene Proteins,Transfection}
}

@article{germainEndoplasmicReticulumBIK2005,
  title = {Endoplasmic Reticulum {{BIK}} Initiates {{DRP1-regulated}} Remodelling of Mitochondrial Cristae during Apoptosis},
  author = {Germain, Marc and Mathai, Jaigi P. and McBride, Heidi M. and Shore, Gordon C.},
  year = {2005},
  month = apr,
  journal = {The EMBO journal},
  volume = {24},
  number = {8},
