  title = {Emi1 Regulates the Anaphase-Promoting Complex by a Different Mechanism than {{Mad2}} Proteins},
  author = {Reimann, J. D. and Gardner, B. E. and {Margottin-Goguet}, F. and Jackson, P. K.},
