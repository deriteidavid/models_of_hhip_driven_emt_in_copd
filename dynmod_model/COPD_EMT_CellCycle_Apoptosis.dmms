  keywords = {Apoptosis,Apoptosis Regulatory Proteins,Calcium,Caspases,Cell Line,Cytochromes c,Endoplasmic Reticulum,Enzyme Activation,GTP Phosphohydrolases,Humans,Intracellular Membranes,Membrane Potentials,Membrane Proteins,Microtubule-Associated Proteins,Mitochondria,Mitochondrial Proteins,Proto-Oncogene Proteins c-bcl-2,Signal Transduction},
  file = {/Users/eregan/Zotero/storage/QYIW23MG/Germain et al. - 2005 - Endoplasmic reticulum BIK initiates DRP1-regulated.pdf}
