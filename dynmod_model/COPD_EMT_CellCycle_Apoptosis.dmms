@article{symmonsWhatLuckGot2016,
  title = {What's {{Luck Got}} to {{Do}} with {{It}}: {{Single Cells}}, {{Multiple Fates}}, and {{Biological Nondeterminism}}},
  shorttitle = {What's {{Luck Got}} to {{Do}} with {{It}}},
