@article{nieRolesMicroRNA34aEpithelial2019,
  title = {Roles of {{MicroRNA-34a}} in {{Epithelial}} to {{Mesenchymal Transition}}, {{Competing Endogenous RNA Sponging}} and {{Its Therapeutic Potential}}},
  author = {Nie, Dongsong and Fu, Jiewen and Chen, Hanchun and Cheng, Jingliang and Fu, Junjiang},
