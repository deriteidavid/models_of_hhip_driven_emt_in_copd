  author = {Tanaka, Hirokazu and Matsumura, Itaru and Ezoe, Sachiko and Satoh, Yusuke and Sakamaki, Toshiyuki and Albanese, Chris and Machii, Takashi and Pestell, Richard G. and Kanakura, Yuzuru},
