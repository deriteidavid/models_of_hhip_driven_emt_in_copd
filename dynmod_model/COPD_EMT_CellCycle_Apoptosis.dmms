@article{leePhysiologicalRestraintBak2016,
  title = {Physiological Restraint of {{Bak}} by {{Bcl-xL}} Is Essential for Cell Survival},
  author = {Lee, Erinna F. and Grabow, Stephanie and Chappaz, Stephane and Dewson, Grant and Hockings, Colin and Kluck, Ruth M. and Debrincat, Marlyse A. and Gray, Daniel H. and Witkowski, Matthew T. and Evangelista, Marco and Pettikiriarachchi, Anne and Bouillet, Philippe and Lane, Rachael M. and Czabotar, Peter E. and Colman, Peter M. and Smith, Brian J. and Kile, Benjamin T. and Fairlie, W. Douglas},
  year = {2016},
