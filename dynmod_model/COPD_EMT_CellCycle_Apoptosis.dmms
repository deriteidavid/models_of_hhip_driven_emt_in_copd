@article{mazzoliniPhosphorylationCDC25ASER2832016,
  title = {Phosphorylation of {{CDC25A}} on {{SER283}} in Late {{S}}/{{G2}} by {{CDK}}/Cyclin Complexes Accelerates Mitotic Entry},
  author = {Mazzolini, Laurent and Broban, Ana{\"i}s and Froment, Carine and {Burlet-Schiltz}, Odile and Besson, Arnaud and Manenti, St{\'e}phane and Dozier, Christine},
  year = {2016},
  month = oct,
  journal = {Cell Cycle (Georgetown, Tex.)},
  volume = {15},
  number = {20},
  pages = {2742--2752},
  issn = {1551-4005},
  doi = {10.1080/15384101.2016.1220455},
  abstract = {The Cdc25A phosphatase is an essential activator of CDK-cyclin complexes at all steps of the eukaryotic cell cycle. The activity of Cdc25A is itself regulated in part by positive and negative feedback regulatory loops performed by its CDK-cyclin substrates that occur in G1 as well as during the G1/S and G2/M transitions. However, the regulation of Cdc25A during G2 phase progression before mitotic entry has not been intensively characterized. Here, we identify by mass spectrometry analysis a new phosphorylation event of Cdc25A on Serine283. Phospho-specific antibodies revealed that the phosphorylation of this residue appears in late S/G2 phase of an unperturbed cell cycle and is performed by CDK-cyclin complexes. Overexpression studies of wild-type and non-phosphorylatable mutant forms of Cdc25A indicated that Ser283 phosphorylation increases the G2/M-promoting activity of the phosphatase without impacting its stability or subcellular localization. Our results therefore identify a new positive regulatory loop between Cdc25A and its CDK-cyclin substrates which contributes to accelerate entry into mitosis through the regulation of Cdc25A activity in G2.},
  langid = {english},
  pmcid = {PMC5053558},
  pmid = {27580187},
  keywords = {activating phosphorylation,Amino Acid Sequence,cdc25 Phosphatases,Cdc25A,CDK-cyclin,cell cycle,Cell Line; Tumor,Cyclin-Dependent Kinases,Cyclins,G2 Phase,G2/M transition,Humans,Intracellular Space,Mass Spectrometry,Mitosis,Mutation,Phosphorylation,Protein Stability,S Phase,Serine},
  file = {/Users/eregan/Zotero/storage/4L293QHJ/Mazzolini et al. - 2016 - Phosphorylation of CDC25A on SER283 in late SG2 b.pdf;/Users/eregan/Zotero/storage/4QKJ4K38/Mazzolini et al. - 2016 - Phosphorylation of CDC25A on SER283 in late SG2 b.pdf}
}

