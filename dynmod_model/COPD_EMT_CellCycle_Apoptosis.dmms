@article{geraldoMicroRNAMiR146b5pRegulates2012,
  title = {{{MicroRNA miR-146b-5p}} Regulates Signal Transduction of {{TGF-$\beta$}} by Repressing {{SMAD4}} in Thyroid Cancer},
  author = {Geraldo, M. V. and Yamashita, A. S. and Kimura, E. T.},
