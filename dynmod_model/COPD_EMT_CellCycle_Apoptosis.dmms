}

@article{leungOverexpressionFoxM1Stimulates2001,
  title = {Over-Expression of {{FoxM1}} Stimulates Cyclin {{B1}} Expression},
  author = {Leung, T. W. and Lin, S. S. and Tsang, A. C. and Tong, C. S. and Ching, J. C. and Leung, W. Y. and Gimlich, R. and Wong, G. G. and Yao, K. M.},
