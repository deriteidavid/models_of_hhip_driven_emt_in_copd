@article{kotaniPKAMPFactivatedPololike1998,
  title = {{{PKA}} and {{MPF-activated}} Polo-like Kinase Regulate Anaphase-Promoting Complex Activity and Mitosis Progression},
  author = {Kotani, S. and Tugendreich, S. and Fujii, M. and Jorgensen, P. M. and Watanabe, N. and Hoog, C. and Hieter, P. and Todokoro, K.},
