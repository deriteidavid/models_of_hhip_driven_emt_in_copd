@article{chenChk1KinaseNegatively2003,
  title = {Chk1 Kinase Negatively Regulates Mitotic Function of {{Cdc25A}} Phosphatase through 14-3-3 Binding},
  author = {Chen, Mei-Shya and Ryan, Christine E. and {Piwnica-Worms}, Helen},
  year = {2003},
  month = nov,
  journal = {Molecular and Cellular Biology},
  volume = {23},
  number = {21},
  pages = {7488--7497},
  issn = {0270-7306},
  doi = {10.1128/mcb.23.21.7488-7497.2003},
  abstract = {The order and fidelity of cell cycle events in mammals is intimately linked to the integrity of the Chk1 kinase-Cdc25A phosphatase pathway. Chk1 phosphorylation targets Cdc25A for destruction and, as shown here, inhibits interactions between Cdc25A and its mitotic substrate cyclin B1-Cdk1. Phosphorylation of Cdc25A on serine 178 and threonine 507 facilitates 14-3-3 binding, and Chk1 phosphorylates both residues in vitro. Mutation of T507 to alanine (T507A) enhanced the biological activity of Cdc25A. Cdc25A(T507A) was more efficient in binding to cyclin B1, activating cyclin B1-Cdk1, and promoting premature entry into mitosis. We propose that the Chk1/Cdc25A/14-3-3 pathway functions to prevent cells from entering into mitosis prior to replicating their genomes to ensure the fidelity of the cell division process.},
  langid = {english},
  pmcid = {PMC207598},
  pmid = {14559997},
  keywords = {14-3-3 Proteins,Amino Acid Sequence,Animals,cdc25 Phosphatases,Cell Line,Checkpoint Kinase 1,Cyclin B,Cyclin B1,DNA Replication,Humans,Mitosis,Molecular Sequence Data,Phosphorylation,Protein Binding,Protein Kinases,Recombinant Fusion Proteins,Serine,Threonine,Tyrosine 3-Monooxygenase},
  file = {/Users/eregan/Zotero/storage/23JPYZNQ/Chen et al. - 2003 - Chk1 kinase negatively regulates mitotic function .pdf;/Users/eregan/Zotero/storage/B2DBX7TE/Chen et al. - 2003 - Chk1 kinase negatively regulates mitotic function .pdf;/Users/eregan/Zotero/storage/JYJ2VWJS/Chen et al. - 2003 - Chk1 kinase negatively regulates mitotic function .pdf}
