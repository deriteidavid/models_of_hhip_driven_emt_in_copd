  keywords = {Alzheimer's disease,amyotrophic lateral sclerosis,grants-supported paper,mitochondrial damage,neural regeneration,neurodegenerative diseases,neuroregeneration,oxidative stress,Parkinson's disease,reactive oxygen species,respiratory chain}
}

