  title = {Mitotic Phosphorylation of Dynamin-Related {{GTPase Drp1}} Participates in Mitochondrial Fission},
  author = {Taguchi, Naoko and Ishihara, Naotada and Jofuku, Akihiro and Oka, Toshihiko and Mihara, Katsuyoshi},
