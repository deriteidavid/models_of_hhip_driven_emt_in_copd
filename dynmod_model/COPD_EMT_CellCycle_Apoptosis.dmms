@article{vigoCDC25APhosphataseTarget1999,
  title = {{{CDC25A}} Phosphatase Is a Target of {{E2F}} and Is Required for Efficient {{E2F-induced S}} Phase},
  author = {Vigo, E. and M{\"u}ller, H. and Prosperini, E. and Hateboer, G. and Cartwright, P. and Moroni, M. C. and Helin, K.},
