  keywords = {Anaphase-Promoting Complex-Cyclosome,Animals,CDC2 Protein Kinase,Cell Cycle,Cell Cycle Proteins,Cyclin A,Cyclin A2,Cyclin-Dependent Kinase 2,DNA Replication,F-Box Proteins,Geminin,HeLa Cells,Humans,Mitosis,Phosphorylation,RNA Interference,Ubiquitin-Protein Ligase Complexes},
