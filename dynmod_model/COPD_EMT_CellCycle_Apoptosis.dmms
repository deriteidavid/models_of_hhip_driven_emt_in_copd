@article{seoaneIntegrationSmadForkhead2004,
  title = {Integration of {{Smad}} and Forkhead Pathways in the Control of Neuroepithelial and Glioblastoma Cell Proliferation},
  author = {Seoane, Joan and Le, Hong-Van and Shen, Lijian and Anderson, Stewart A. and Massagu{\'e}, Joan},
