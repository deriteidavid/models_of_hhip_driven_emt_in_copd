@article{lobjoisPololikeKinaseRegulates2009,
  title = {The Polo-like Kinase 1 Regulates {{CDC25B-dependent}} Mitosis Entry},
  author = {Lobjois, Valerie and Jullien, Denis and Bouch{\'e}, Jean-Pierre and Ducommun, Bernard},
