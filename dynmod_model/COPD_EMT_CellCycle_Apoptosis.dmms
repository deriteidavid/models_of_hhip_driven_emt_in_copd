@article{manonReleaseCytochromeDecrease1997,
  title = {Release of Cytochrome c and Decrease of Cytochrome c Oxidase in {{Bax-expressing}} Yeast Cells, and Prevention of These Effects by Coexpression of {{Bcl-xL}}},
  author = {Manon, S. and Chaudhuri, B. and Gu{\'e}rin, M.},
